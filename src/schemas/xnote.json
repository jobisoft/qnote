[{
	"namespace": "xnote",
	"functions": [{
		"name": "getPrefs",
		"type": "function",
		"description": "Get preferences",
		"async": true,
		"parameters": []
	},{
		"name": "getStoragePath",
		"type": "function",
		"description": "Get XNote storage folder",
		"async": true,
		"parameters": [{
			"name": "path",
			"optional": true,
			"type": "string",
			"description": "Path from prefs (may contain [ProfD]) or null if XNote folder exists under profile dir"
		}]
	},{
		"name": "getAllKeys",
		"type": "function",
		"description": "Returns list of XNote keys in a folder",
		"async": true,
		"parameters": [{
			"name": "root",
			"type": "string",
			"description": "Path to XNote folder"
		}]
	},{
		"name": "loadNote",
		"type": "function",
		"description": "Loads XNote from file",
		"async": true,
		"parameters": [{
			"name": "root",
			"type": "string",
			"description": "Path to XNote folder"
		},{
			"name": "keyId",
			"type": "string",
			"description": "XNote key"
		}]
	},{
		"name": "deleteNote",
		"type": "function",
		"description": "Delete XNote",
		"async": true,
		"parameters": [{
			"name": "root",
			"type": "string",
			"description": "Path to XNote folder"
		},{
			"name": "keyId",
			"type": "string",
			"description": "XNote key"
		}]
	},{
		"name": "saveNote",
		"type": "function",
		"description": "Saves xnote into file",
		"async": true,
		"parameters": [{
			"name": "root",
			"type": "string",
			"description": "Path to XNote folder"
		},{
			"name": "keyId",
			"type": "string",
			"description": "XNote key"
		},{
			"name": "data",
			"type": "object",
			"properties": {
				"left": {
					"type": "integer",
					"optional": true,
					"default": 0,
					"description": "Windows left position"
				},
				"top": {
					"type": "integer",
					"optional": true,
					"default": 0,
					"description": "Windows top position"
				},
				"width": {
					"type": "integer",
					"optional": true,
					"default": 0,
					"description": "Windows width"
				},
				"height": {
					"type": "integer",
					"optional": true,
					"default": 0,
					"description": "Windows height"
				},
				"text": {
					"type": "string",
					"optional": true,
					"default": "",
					"description": "Note text"
				},
				"ts": {
					"type": "integer",
					"optional": true,
					"default": 0,
					"description": "Modification timestamp"
				}
			},
			"default": {},
			"description": "Data object"
		}]
	}]
}]