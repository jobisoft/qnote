[{
	"namespace": "qpopup",
	"types": [{
		"id": "QPopupOptions",
		"type": "object",
		"properties": {
			"focused": {
				"type": "boolean",
				"optional": true
			},
			"top": {
				"type": "integer",
				"optional": true
			},
			"left": {
				"type": "integer",
				"optional": true
			},
			"offsetTop": {
				"type": "integer",
				"optional": true
			},
			"offsetLeft": {
				"type": "integer",
				"optional": true
			},
			"width": {
				"type": "integer",
				"optional": true
			},
			"height": {
				"type": "integer",
				"optional": true
			},
			"anchor": {
				"type": "string",
				"optional": true
			},
			"anchorPlacement": {
				"type": "string",
				"optional": true
			},
			"title": {
				"type": "string",
				"optional": true
			},
			"text": {
				"type": "string",
				"optional": true
			},
			"placeholder": {
				"type": "string",
				"optional": true
			},
			"focusOnDisplay": {
				"type": "boolean",
				"optional": true
			},
			"enableSpellChecker": {
				"type": "boolean",
				"optional": true
			}
		}
	}],
	"functions": [{
		"name": "setDebug",
		"type": "function",
		"description": "Enable/Disable debug messages",
		"async": true,
		"parameters": [{
			"name": "on",
			"type": "boolean",
			"optional": true,
			"default": false
		}]
	},{
		"name": "get",
		"type": "function",
		"async": "callback",
		"parameters": [{
			"type": "integer",
			"name": "id",
			"minimum": 1
		}]
	},{
		"name": "update",
		"type": "function",
		"async": "callback",
		"parameters": [{
			"name": "id",
			"type": "integer",
			"minimum": 1
		},{
			"name": "options",
			"$ref": "QPopupOptions"
		}]
	},{
		"name": "remove",
		"type": "function",
		"async": "callback",
		"parameters": [{
			"name": "id",
			"type": "integer",
			"minimum": 1
		}]
	},{
		"name": "create",
		"type": "function",
		"async": "callback",
		"parameters": [{
			"type": "integer",
			"name": "windowId",
			"minimum": 1
		},{
			"name": "options",
			"$ref": "QPopupOptions"
		}]
	},{
		"name": "pop",
		"type": "function",
		"async": "callback",
		"parameters": [{
			"type": "integer",
			"name": "id",
			"minimum": 1
		}]
	}],
	"events": [{
		"name": "onCreated",
		"type": "function",
		"description": "Fired when a popup is created.",
		"parameters": [{
			"name": "options",
			"$ref": "QPopupOptions",
			"description": "Details of the popup that was created."
		}]
	},{
		"name": "onRemoved",
		"type": "function",
		"description": "Fired when a popup is removed.",
		"parameters": [{
			"name": "options",
			"$ref": "QPopupOptions",
			"description": "Details of the popup that was removed."
		}]
	},{
		"name": "onMove",
		"type": "function",
		"description": "Fired when a popup is moved.",
		"parameters": [{
			"name": "options",
			"$ref": "QPopupOptions",
			"description": "Details of the popup that was removed."
		}]
	},{
		"name": "onResize",
		"type": "function",
		"description": "Fired when a popup is resized.",
		"parameters": [{
			"name": "options",
			"$ref": "QPopupOptions",
			"description": "Details of the popup that was resized."
		}]
	}]
}]
