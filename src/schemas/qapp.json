[{
	"namespace": "qnote",
	"types": [{
		"id": "NoteData",
		"name": "data",
		"type": "object",
		"description": "Note data (keep in sync with qnote.json)",
		"properties": {
			"text": {
				"type": "string",
				"optional": true,
				"description": "Note text"
			},
			"left": {
				"type": "integer",
				"optional": true,
				"description": "Windows left position"
			},
			"top": {
				"type": "integer",
				"optional": true,
				"description": "Windows top position"
			},
			"width": {
				"type": "integer",
				"optional": true,
				"description": "Windows width"
			},
			"height": {
				"type": "integer",
				"optional": true,
				"description": "Windows height"
			},
			"ts": {
				"type": "integer",
				"optional": true,
				"description": "Modification timestamp"
			}
		}
	}]
},{
