{
	"manifest_version": 2,
	"name": "QNote",
	"author": "Martins Lazdans",
	"default_locale": "en",
	"description": "__MSG_description__",
	"version": "0.14.8",
	"applications": {
		"gecko": {
			"id": "qnote@dqdp.net",
			"strict_min_version": "128.0",
			"strict_max_version": "139.*"
		}
	},
	"icons": {
		"32": "images/icons/qnote.svg",
		"48": "images/icons/qnote.svg",
		"64": "images/icons/qnote.svg"
	},
	"permissions": [
		"messagesTagsList",
		"messagesRead",
		"messagesUpdate",
		"storage",
		"unlimitedStorage",
		"downloads",
		"menus",
		"accountsRead",
		"scripting",
		"messagesModify"
	],
	"background": {
		"page": "background.html"
	},
	"options_ui": {
		"page": "html/options.html"
	},
	"message_display_action": {
		"default_title": "QNote",
		"default_icon": "images/icons/qnote-disabled.svg"
	},
	"browser_action": {
		"default_title": "QNote",
		"default_icon": "images/icons/qnote-disabled.svg"
	},
	"commands": {
		"qnote": {
			"suggested_key": {
				"default": "Alt+Q"
			},
			"description": "__MSG_toggleqnote__"
		}
	},
	"experiment_apis": {
		"qapp": {
			"schema": "schemas/qapp.json",
			"parent": {
				"scopes": ["addon_parent"],
				"paths": [["qapp"]],
				"script": "api/qapp.js"
			}
		},
		"legacy": {
			"schema": "schemas/legacy.json",
			"parent": {
				"scopes": ["addon_parent"],
				"paths": [["legacy"]],
				"script": "api/legacy.js"
			}
		},
		"xnote": {
			"schema": "schemas/xnote.json",
			"parent": {
				"scopes": ["addon_parent"],
				"paths": [["xnote"]],
				"script": "api/xnote.js"
			}
		},
		"qnote": {
			"schema": "schemas/qnote.json",
			"parent": {
				"scopes": ["addon_parent"],
				"paths": [["qnote"]],
				"script": "api/qnote.js"
			}
		},
		"qpopup": {
			"schema": "schemas/qpopup.json",
			"parent": {
				"scopes": ["addon_parent"],
				"paths": [["qpopup"]],
				"script": "api/qpopup.js"
			}
		},
		"ResourceUrl": {
			"schema": "schemas/ResourceUrl.json",
			"parent": {
				"scopes": ["addon_parent"],
				"paths": [["ResourceUrl"]],
				"script": "api/ResourceUrl.js"
			}
		},
		"LegacyCSS": {
			"schema": "schemas/LegacyCSS.json",
			"parent": {
				"scopes": ["addon_parent"],
				"paths": [["LegacyCSS"]],
				"script": "api/LegacyCSS.js"
			}
		}
	}
}
